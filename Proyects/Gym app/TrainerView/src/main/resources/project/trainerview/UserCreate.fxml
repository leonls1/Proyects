<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="240.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.trainerview.controller.UserCreateController">
   <children>
      <Label layoutX="136.0" layoutY="14.0" text="Crear nuevo usuario" />
      <HBox layoutX="21.0" layoutY="52.0" prefHeight="100.0" prefWidth="325.0">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="115.0" prefWidth="118.0" spacing="10.0">
               <children>
                  <Label text="Nombre(*)" />
                  <Label text="Apellido" />
                  <Label text="DNI(*)" />
                  <Label prefWidth="100.0" text="Vencimiento cuota" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="100.0" prefWidth="221.0" spacing="5.0">
               <children>
                  <TextField fx:id="txtFName" />
                  <TextField fx:id="txtFSurname" />
                  <Spinner fx:id="spnId" prefHeight="25.0" prefWidth="226.0" />
                  <DatePicker fx:id="dtpExpirationFee" prefHeight="25.0" prefWidth="282.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Button fx:id="btnCreate" layoutX="302.0" layoutY="196.0" mnemonicParsing="false" onAction="#btnEvent" text="Crear" />
      <Button fx:id="btnMenu" layoutX="15.0" layoutY="196.0" mnemonicParsing="false" onAction="#btnEvent" text="Menu" />
      <Label layoutX="136.0" layoutY="213.0" text="(*): campo obligatorio" />
   </children>
</AnchorPane>
