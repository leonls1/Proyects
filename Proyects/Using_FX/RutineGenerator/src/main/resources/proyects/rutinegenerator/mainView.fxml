<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="466.0" prefWidth="745.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proyects.rutinegenerator.controller.MainViewController">
   <children>
      <Label layoutX="343.0" layoutY="40.0" text="Version 0.1" />
      <Label layoutX="271.0" text="Rutine Generator">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <TableView fx:id="tableRutine" layoutX="71.0" layoutY="140.0" onMouseClicked="#mouseEvent" prefHeight="200.0" prefWidth="520.0">
        <columns>
          <TableColumn fx:id="exerciceCol" prefWidth="360.0" text="Ejercicio" />
          <TableColumn fx:id="serieCol" prefWidth="74.0" text="Series" />
            <TableColumn fx:id="repCol" prefWidth="85.0" text="Repeticiones" />
        </columns>
      </TableView>
      <Button fx:id="btnGenerate" layoutX="591.0" layoutY="371.0" mnemonicParsing="false" onAction="#btnEvent" text="Generar" />
      <ComboBox fx:id="cboRutineType" layoutX="71.0" layoutY="78.0" onAction="#cboEvent" prefWidth="150.0" promptText="Tipo de rutina" />
   </children>
</AnchorPane>
